   "source": [
    "def digit_sum(n):\n",
    "    ns = n\n",
    "    ss = 0\n",
    "    while ns != 0:\n",
    "        ss += ns % 10\n",
    "        ns = ns // 10\n",
    "    return ss"
   ]
  },
